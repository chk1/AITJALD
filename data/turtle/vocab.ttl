lodcom:Teilbereich  rdfs:label  "Sub Borough"@en , "Teilbereich"@de ;
        rdfs:subClassOf  dbo:AdministrativeRegion .

lodcom:upperAdministrativeLevel
        a             rdf:Property ;
        rdfs:comment  "relates an administrative area to the next higher administrative level" ;
        rdfs:domain   dbo:AdministrativeRegion ;
        rdfs:range    dbo:AdministrativeRegion .

lodcom:districtId  a  rdf:Property ;
        rdfs:domain  lodcom:Stadtteil ;
        rdfs:range   xsd:integer .

lodcom:Stadtteil  rdfs:label  "City District"@en , "Stadtteil"@de ;
        rdfs:subClassOf  dbo:AdministrativeRegion .

lodcom:Stadtbezirk  rdfs:label  "Borough"@en , "Stadtbezirk"@de ;
        rdfs:subClassOf  dbo:AdministrativeRegion .
